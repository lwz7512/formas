  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "release": "npm run build && npm publish",
    "start": "dumi dev",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "xlsx": ">=0.17.0"
  },
  "devDependencies": {
    "antd": "^4.24.16",
    "dumi": "^1.0.10",
    "father-build": "^1.17.2",
    "@formas/form-render": "workspace:*",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^17.0.1",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "antd": "^4.24.16",
    "react": ">=16.8.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "group": "form-render"
